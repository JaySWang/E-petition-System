<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="struts2" extends="struts-default">

		<action name="showProposal" class="showAction" method="showProposals" >
		
			<result name="success"  >/JSP/proposals/iraq.jsp</result>
			<result name="error" >/JSP/proposals/error.jsp</result>
		</action>

	
		<action name="addProposal" class="addProposalAction" >
		
			<result name="success"  >/JSP/governmentOfficer/addProposal.jsp</result>
			<result name="error" >/JSP/proposals/error.jsp</result>
		</action>
	


	</package>

	
	
	
</struts>
